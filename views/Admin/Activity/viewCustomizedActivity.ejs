
<%- include('../common/header') %>
<%- include('../common/sidebar') %>

<!-- BEGIN: Content-->
<div class="app-content content ">
  <div class="content-wrapper container-xxl p-0">
    <div class="content-body">
      <section id="multiple-column-form">
        <div class="row">
          <div class="col-md-12 col-12">
            <div class="card-body">
              <div class="col-md-10 col-12 ">
                <div class="card">
                  <div class="card-header">
                    <h4 class="card-title"> View customized activity</h4>
                  </div>
                  <br>
                    <form class="form form-vertical" id="" action="/activity_List" method="POST" enctype="multipart/form-data">
                        <div class="mb-1 row">
                            <div class="col-sm-2">
                                <label class="col-form-label new_lable" for="name">User Name</label>
                            </div>
                            <div class="col-sm-10">
                                <input type="text" id="" class="form-control" name="name" value="<%=activitydetail.userId.name%>" disabled>
                            </div>
                        </div>
                        <div class="mb-1 row">
                            <div class="col-sm-2">
                                <label class="col-form-label new_lable" for="activity_name"> Activity Name</label>
                            </div>
                            <div class="col-sm-10">
                                 <input type="text" id="" class="form-control" name="activity_name" value="<%=activitydetail.activity_name%>" disabled>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="col-sm-9 offset-sm-5">
                                <a href="/activityList" class="btn btn-primary float-right" title="Edit Profile"> Back </a>  
                            </div>
                        </div>
                    </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>
  </div>
</div>
<div class="sidenav-overlay"></div>
<div class="drag-target"></div>
<%- include('../common/footer') %>



